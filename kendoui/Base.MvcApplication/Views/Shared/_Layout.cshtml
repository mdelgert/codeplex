<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="/favicon.ico">
    <title>@ViewBag.Title</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Content/Bootstrap/bootstrap-3.1.1/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Bootstrap theme -->
    <link href="~/Content/Bootstrap/bootstrap-3.1.1/dist/css/bootstrap-theme.min.css" rel="stylesheet">

    <!-- Documentation extras -->
    <link href="~/Content/Bootstrap/bootstrap-3.1.1/docs/assets/css/docs.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/Content/Site.css" rel="stylesheet">

    <!-- Just for debugging purposes. Don't actually copy this line! -->
    <!--[if lt IE 9]><script src="/Content/Bootstrap/bootstrap-3.1.1/docs/assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="/Content/HTML5shim/html5shiv.js"></script>
      <script src="/Content/HTML5shim/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", required: false)
</head>
<body role="document">
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">Kendoui</a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Local HTML Docs<b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Bootstrap", "Index", "Bootstrap")</li>
                            <li><a href="/Content/KendoUI/telerik.kendoui.web.2014.1.318.open-source/examples/index.html">KendoUi</a></li>
                            <li><a href="/Content/jQueryMobile/jquery.mobile-1.4.2/demos/index.html">jQuery Mobile</a></li>
                            <li><a href="/Content/jQueryUI/jquery-ui-1.10.4/demos/index.html">jQuery UI</a></li>
                        </ul>
                    </li>
                    <li>@Html.ActionLink("External Links", "Links", "Home")</li>
                    <li>@Html.ActionLink("Task item", "CRUD", "TaskItem")</li>
                    <li>@Html.ActionLink("Log Api", "Index", "LogApi")</li>
                    <li>@Html.ActionLink("Login", "Login", "Account")</li>
                    <li>@Html.ActionLink("Bootstrap Table Pagination", "TablePagination", "Bootstrap")</li>
                    <li>@Html.ActionLink("SqliteJS Example", "Index", "SqliteJS")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container theme-showcase" role="main">
        @RenderBody()
    </div>
    <!-- Bootstrap core JavaScript. Placed at the end of the document so the pages load faster -->
    <script src="/Content/jQuery/jquery-1.11.0/jquery-1.11.0.min.js"></script>
    <script src="/Content/Bootstrap/bootstrap-3.1.1/dist/js/bootstrap.min.js"></script>
    <script src="/Content/Bootstrap/bootstrap-3.1.1/docs/assets/js/docs.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>

@*@Html.Partial("~/Views/Shared/_SourceViewer.cshtml")*@

